datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id             Int       @id @default(autoincrement())
  firstName      String
  lastName       String
  userName       String    @unique
  email          String    @unique
  phoneNo        String
  password       String
  createdAt      DateTime? @default(now()) @map("createdAt") @db.Timestamp(0)
  lastModifiedAt DateTime  @updatedAt
}